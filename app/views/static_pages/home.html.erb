<%= form_with url: static_pages_result_url, local: true do |form| %>
  <%= form.label :性別 %>
  <%=  select :page, :gender, {"男" => "male", "女" => "female"} %>
  <br></br>
  <%= form.label :身長 %>
  <%= form.text_field :height %>
  <br></br>
  <%= form.label :体重 %>
  <%= form.text_field :weight %>
  <br></br>
  <%= form.label :年齢 %>
  <%= form.text_field :old %>
  <br></br>
  <%= form.label :アクティブ度 %>
  <%=  select :page, :activity, {"低い" => "low", "まあまあ" => "mid", "かなり高い" => "high"} %>
  <br></br>
  <%= form.label :目的 %>
  <%=  select :page, :purpose, {"減量" => "loss", "維持" => "maintenance", "増量" => "increase"} %>
  <br></br>
  <%= form.submit "results"%>
<% end %>